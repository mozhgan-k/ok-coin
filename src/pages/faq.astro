---
import FAQPage from '../components/FAQPage.vue';
import '../assets/styles/main.css';
const response = await fetch('https://azapi.ok-ex.io/server/api/support/faq');
const faqData = response.ok ? await response.json() : null;
const props = { faqs: faqData.data };
---

<html>
  <head>
    <meta charset="UTF-8">
    <title>FAQ</title>
  </head>
  <body>
    <div id="faq">
      <!-- SSR render of Vue component -->
      <FAQPage {...props} client:load /> 
    </div>
  </body>
</html>
